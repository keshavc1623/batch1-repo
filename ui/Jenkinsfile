                echo 'checkout..'
            }
        }
        stage('install dependencies') {
            steps {
                echo 'install dependencies..'
            }
        }
        stage('building') {
            steps {
                echo 'building....'
            }
        }
        stage('test') {
            steps {
                echo 'testing....'
            }
        }
         stage('deploy') {
            steps {
                echo 'deploying....'
            }
        }
    }
}